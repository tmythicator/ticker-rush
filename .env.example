# Server Configuration
SERVER_PORT=8081
CLIENT_PORT=5173

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379

# Database Configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=ticker_rush
POSTGRES_PORT=5432
POSTGRES_HOST=localhost

# Ticker Selection
# Comma-separated list of symbols.
# Prefix with 'CG:' for CoinGecko crypto IDs (e.g., CG:bitcoin, CG:ethereum).
# Prefix with 'FH:' for Finnhub symbols (e.g., FH:AAPL, FH:BINANCE:BTCUSDT).
# Standard symbols without prefix default to Finnhub.
TICKERS=FH:AAPL,FH:BINANCE:BTCUSDT,CG:bitcoin,CG:ethereum

# Finnhub API Configuration
FINNHUB_API_KEY=your_finnhub_key_here
FINNHUB_FETCH_INTERVAL=10s
FINNHUB_TIMEOUT=10s

# CoinGecko API Configuration
COINGECKO_API_KEY=
COINGECKO_FETCH_INTERVAL=10s
COINGECKO_TIMEOUT=10s

# Auth
JWT_SECRET=super_secret_key